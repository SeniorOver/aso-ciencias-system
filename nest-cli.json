{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/aso-ciencias-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/aso-ciencias-system/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/aso-ciencias-system",
  "projects": {
    "analytics-service": {
      "type": "application",
      "root": "apps/analytics-service",
      "entryFile": "main",
      "sourceRoot": "apps/analytics-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics-service/tsconfig.app.json"
      }
    },
    "aso-ciencias-system": {
      "type": "application",
      "root": "apps/aso-ciencias-system",
      "entryFile": "main",
      "sourceRoot": "apps/aso-ciencias-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/aso-ciencias-system/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "inventory-service": {
      "type": "application",
      "root": "apps/inventory-service",
      "entryFile": "main",
      "sourceRoot": "apps/inventory-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory-service/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "sales-service": {
      "type": "application",
      "root": "apps/sales-service",
      "entryFile": "main",
      "sourceRoot": "apps/sales-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sales-service/tsconfig.app.json"
      }
    }
  }
}